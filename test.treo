import reo.fifo1;

test(a,b) {


	fifo = <k:int>(a?, b!) { 
	
		if (k <= 0) { 
			a = b 
		} else {
			fifo[k](a,b)
		}
	
		for i = 0..k {
			if (i == 0) {
				fifo[0] = (a?,b!) {
					a = b
				}
			} else (i > 0) {
				fifo[i] = (a?,b!) {
					fifo1(a,x) 
					fifo[i-1](x,b)
				}
			}
		}
	}

	//x=<k>(){for i = 1..k { fifo1(a[i],b) } }
	//x<2>()

	fifo<3>(a,b)

}
